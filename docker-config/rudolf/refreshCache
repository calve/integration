echo "Refreshing Cache in Rudolf..." | tee -a /var/www/refreshCache.log
cd /var/www/rudolf && \
       php artisan cache:clear | tee -a /var/www/refreshCache.log && \
       php artisan search:load | tee -a /var/www/refreshCache.log && \
       php artisan model:load global | tee -a /var/www/refreshCache.log
cd /var/www/rudolf && \
    chmod -R a+rwx .
echo "Cache in Rudolf refreshed." | tee -a /var/www/refreshCache.log
